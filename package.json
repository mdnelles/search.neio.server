{
  "name": "ver1.02",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "12.14.0",
    "npm": "6.13.4"
  },
  "main": "server.js",
  "scripts": {
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js --ignore client",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\"  --disableHostCheck true",
    "do-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix && npm run build --prefix ../client",
    "heroku-postbuild": "cd ../client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "adminStart": true,
  "keywords": [
    "nodejs",
    "jwt",
    "passport",
    "express"
  ],
  "author": "JSmart",
  "license": "ISC",
  "dependencies": {
    "array-sort": "*",
    "bcrypt": "^3.0.8",
    "body-parser": "*",
    "busboy": "^0.3.1",
    "concurrently": "^5.1.0",
    "connect-busboy": "0.0.2",
    "cookie-parser": "*",
    "cors": "*",
    "express": "*",
    "express-fileupload": "^1.1.6",
    "express-session": "*",
    "file-type": "^14.1.1",
    "flatted": "*",
    "formidable": "^1.2.1",
    "fs-extra": "^8.1.0",
    "http": "0.0.0",
    "jquery": "^3.4.1",
    "jsonwebtoken": "*",
    "multer": "^1.4.2",
    "mysql2": "*",
    "read-chunk": "^3.2.0",
    "sequelize": "*",
    "serve": "*",
    "shelljs": "*",
    "util": "^0.12.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "nodemon": "*"
  }
}
