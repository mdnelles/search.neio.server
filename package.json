{
   "name": "search-code-base-server",
   "version": "1.1.03",
   "description": "NodeJS TypeScript Server",
   "type": "module",
   "engines": {
      "node": "^18.0.0",
      "npm": "^8.0.0"
   },
   "main": "server.js",
   "scripts": {
      "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
      "client-install": "npm install --prefix client",
      "start": "node ./src/server.js",
      "server": "nodemon ./src/server.js --ignore client",
      "lint:init": "./node_modules/.bin/eslint --init",
      "lint": "eslint ./src/  --ext .js",
      "lint:fix": "eslint --fix ./src/  --ext .js"
   },
   "adminStart": true,
   "keywords": [
      "nodejs",
      "jwt",
      "passport",
      "express"
   ],
   "author": "JSmart",
   "license": "ISC",
   "dependencies": {
      "array-sort": "^1.0.0",
      "bcrypt": "^5.1.0",
      "body-parser": "*",
      "compression": "^1.7.4",
      "concurrently": "*",
      "cookie-parser": "^1.4.6",
      "cors": "*",
      "dotenv": "^16.0.3",
      "express": "*",
      "express-fileupload": "^1.4.0",
      "express-session": "^1.17.3",
      "flatted": "^3.2.7",
      "formidable": "^2.0.1",
      "fs-extra": "^11.1.0",
      "helmet": "^6.0.1",
      "jquery": "^3.6.1",
      "jsonwebtoken": "^8.5.1",
      "morgan": "^1.10.0",
      "mysql2": "^2.3.3",
      "path": "^0.12.7",
      "sequelize": "^6.29.0",
      "serve": "^14.1.2",
      "shelljs": "^0.8.5",
      "url": "^0.11.0",
      "uuid": "^9.0.0"
   },
   "devDependencies": {
      "eslint": "^8.28.0",
      "nodemon": "^2.0.20"
   }
}
